{
  "include": ["src/**/*.ts"],
  "compilerOptions": {
    "jsx": "react-native",
    "lib": ["es2015", "es2016", "esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es5"
  },
  "exclude": ["node_modules"]
}
